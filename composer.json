{
  "name": "quantumtecnology/nfse-nacional",
  "license": "MIT",
  "description": "Pacote para geração de NFSe Nacional usando componentes NFePHP (https://github.com/nfephp-org)",
  "keywords": [
    "nfse-nacional",
    "nfse",
    "mei"
  ],
  "autoload": {
    "psr-4": {
      "Hadder\\NfseNacional\\": "src/"
    },
    "files": [
      "Helpers.php"
    ]
  },
  "authors": [
    {
      "name": "Fernando Friedrich",
      "email": "fernando@hadder.com.br"
    },
    {
      "name": "Luis Gustavo Santarosa Pinto",
      "email": "gustavo-computacao@hotmail.com"
    },
    {
      "name": "Bruno Alvim",
      "email": "bruno.alvim@ezcompany.com.br"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.2",
    "symfony/var-dumper": "^7.1 | ^6.0",
    "tecnickcom/tcpdf": "^6.7",
    "nfephp-org/sped-common": "^5.1",
    "nfephp-org/sped-da": "dev-master",
    "ext-dom": "*",
    "nesbot/carbon": "^3.8 | ^2.0",
    "ext-curl": "*",
    "ext-zlib": "*",
    "ext-openssl": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.89",
    "laravel/pint": "^1.0"
  },
  "config": {
    "optimize-autoloader": true
  },
  "prefer-stable": true
}
